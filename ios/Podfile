# Uncomment this line to define a global platform for your project
platform :ios, '13.0'

# CocoaPods master repo
source 'https://cdn.cocoapods.org/'

# Flutter's Podfile (automatically generated)
flutter_application_path = '../..'
load File.join(flutter_application_path, 'packages', 'flutter_tools', 'bin', 'podhelper.rb')

target 'Runner' do
  # Pods for Runner
  use_frameworks!
  # Flutter-specific dependencies
  install_all_flutter_pods(flutter_application_path)

  # Add any other iOS dependencies below this line:
  # pod 'SomeOtherPod', '~> 1.0'
end
